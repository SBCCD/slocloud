{
    "name": "lawsonry/slocloud",
    "description": "A Student Learning Objectives (SLO) Reporting Tool",
    "require": {
        "jquery/jquery": "1.11.2",
        "wycats/handlebars.js": "2.0.0",
        "twbs/bootstrap": "~3.3",
        "fortawesome/font-awesome": "~4.3",
        "slim/slim": "~2.4",
        "monolog/monolog": "~1.11",
        "slim/views": "~0.1",
        "twig/twig": "~1.6",
        "robmorgan/phinx": "~0.3",
        "hakimel/ladda": "0.9.4",
        "jonnyreeves/js-logger": "v0.9.14",
        "paulmillr/console-polyfill": "0.1.2",
        "swiftmailer/swiftmailer": "~5.3",
        "moment-git/moment": "2.9.0",
        "johnculviner/jquery.fileDownload": "v1.4.3",
        "flesler-git/jquery.scrollto": "1.4.14",
        "phing/phing": "~2.10",
        "less.php/less.php": "v1.7.0.2",
        "doctrine/orm": "~2.4",
        "beberlei/assert": "~2.3",
        "scottjehl/Respond": "1.4.2",
        "aFarkas/html5shiv": "3.7.2"
    },
    "require-dev": {
      "squizlabs/php_codesniffer": "~1.5",
      "phpunit/phpunit": "~4.5",
      "phpunit/dbunit": "~1.3"
    },
    "autoload": {
      "psr-4": {
        "SLOCloud\\" : "src/"
      }
    },
    "repositories": [
      {
        "type": "package",
        "package": {
          "name": "scottjehl/Respond",
          "version": "1.4.2",
          "dist": {
            "url": "https://github.com/scottjehl/Respond/archive/1.4.2.zip",
            "type": "zip"
          }
        }
      },
      {
        "type": "package",
        "package": {
          "name": "aFarkas/html5shiv",
          "version": "3.7.2",
          "dist": {
            "url": "https://github.com/aFarkas/html5shiv/archive/3.7.2.zip",
            "type": "zip"
          }
        }
      },
      {
        "type": "package",
        "package": {
          "name": "moment-git/moment",
          "version": "2.9.0",
          "dist": {
            "url": "https://github.com/moment/moment/archive/2.9.0.zip",
            "type": "zip"
          }
        }
      },
      {
        "type": "package",
        "package": {
          "name": "flesler-git/jquery.scrollto",
          "version": "1.4.14",
          "dist": {
            "url": "https://github.com/flesler/jquery.scrollTo/archive/1.4.14.zip",
            "type": "zip"
          }
        }
      },
      {
        "type": "package",
        "package": {
          "name": "wycats/handlebars.js",
          "version": "2.0.0",
          "dist": {
            "url": "http://builds.handlebarsjs.com.s3.amazonaws.com/handlebars-v2.0.0.js",
            "type": "file"
          }
        }
      },
      {
        "type": "package",
        "package": {
          "name": "jquery/jquery",
          "version": "1.11.2",
          "dist": {
            "url": "https://github.com/jquery/jquery/archive/1.11.2.zip",
            "type": "zip"
          }
        }
      },
      {
        "type": "package",
        "package": {
          "name": "johnculviner/jquery.fileDownload",
          "version": "v1.4.3",
          "dist": {
            "url": "https://raw.githubusercontent.com/johnculviner/jquery.fileDownload/0f73be57ba307fd1a689c7266f1d7d1a3d981146/src/Scripts/jquery.fileDownload.js",
            "type":"file"
          }
        }
      },
      {
        "type": "package",
        "package": {
          "name": "paulmillr/console-polyfill",
          "version": "0.1.2",
          "dist": {
            "url": "https://github.com/paulmillr/console-polyfill/archive/0.1.2.zip",
            "type":"zip"
          }
        }
      },
      {
        "type": "package",
        "package": {
          "name": "hakimel/ladda",
          "version": "0.9.4",
          "dist": {
            "url": "https://github.com/hakimel/Ladda/archive/0.9.4.zip",
            "type":"zip"
          }
        }
      },
      {
        "type": "package",
        "package": {
          "name": "jonnyreeves/js-logger",
          "version": "v0.9.14",
          "dist": {
            "url": "https://github.com/jonnyreeves/js-logger/archive/0.9.14.zip",
            "type": "zip"
          }
        }
      }
    ],
